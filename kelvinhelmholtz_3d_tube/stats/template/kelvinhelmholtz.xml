  <boundary>periodic</boundary>
  <flux>hll3</flux>
  <endTime>2</endTime>
  <equation>euler3</equation>
  <equationParameters>
    <gamma>1.4</gamma>
  </equationParameters>
  <reconstruction>weno2</reconstruction>
  <cfl>auto</cfl>
  <integrator>auto</integrator>
  <functionals>


  </functionals>
  <initialData>
    <python>kelvinhelmholtz.py</python>
    <parameters>
      <parameter>
        <name>a</name>
        <length>60</length>
        <values><value>0</value></values>
      </parameter>
    </parameters>
  </initialData>
  <diffusion>
    <name>none</name>
    <reconstruction>none</reconstruction>
  </diffusion>
</fvm>
