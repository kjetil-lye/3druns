<config>
<fvm>
  <name>
    taylorgreen
  </name>
  <platform>cuda</platform>
  <grid>
    <lowerCorner>0 0 0</lowerCorner>
    <upperCorner>1 1 1</upperCorner>
    <dimension>64 64 64</dimension>
  </grid>
  <boundary>periodic</boundary>
  <flux>HLL3</flux>
  <endTime>0.1</endTime>
  <equation>euler3</equation>
  <reconstruction>weno2</reconstruction>
  <cfl>auto</cfl>
  <integrator>rungekutta4</integrator>
  <initialData>
    <python>taylorgreen.py</python>
  </initialData>
  <writer>
    <type>netcdf</type>
    <basename>tg</basename>
    <numberOfSaves>10</numberOfSaves>
  </writer>
</fvm>
<uq>
 <samples>4</samples>
  <generator>auto</generator>
  <parameters>
    <parameter>
      <name>a</name>
      <length>40</length>
      <type>uniform</type>
    </parameter>
  </parameters>
  <stats>

  </stats>
</uq>
</config>
