<config>
  <fvm>
    <name>
      cloudshock
    </name>
    <platform>cuda</platform>
    <grid>
      <lowerCorner>0 0 0</lowerCorner>
      <upperCorner>1 1 1</upperCorner>
      <dimension>128 128 128</dimension>
    </grid>
    <boundary>neumann</boundary>
    <flux>hll3</flux>
    <endTime>0.06</endTime>
    <equation>euler3</equation>
    <equationParameters>
      <gamma>1.4</gamma>
    </equationParameters>
    <reconstruction>wenof2</reconstruction>
    <cfl>auto</cfl>
    <integrator>auto</integrator>
    <functionals>


    </functionals>
    <initialData>
      <python>cloudshock.py</python>
      <parameters>
	<parameter>
          <name>X</name>
          <length>10</length>
          <values><value>0</value></values>
	</parameter>
      </parameters>
    </initialData>
    <diffusion>
      <name>none</name>
      <reconstruction>none</reconstruction>
    </diffusion>
  </fvm>
  <uq>
    <samples>128</samples>
    <generator>auto</generator>
    <parameters>
      <parameter>
	<name>X</name>
	<length>10</length>
	<type>uniform</type>
      </parameter>
    </parameters>
    <stats>
      <stat>
	<name>
	  meanvar
	</name>
	<numberOfSaves>1</numberOfSaves>
	<writer>
	  <type>netcdf</type>
	  <basename>cloudshock</basename>
	</writer>
      </stat>
      <stat>
	<name>
	  structure_cube
	</name>
	<numberOfSaves>2</numberOfSaves>
	<numberOfH>4</numberOfH>
	<p>1</p>
	<writer>
	  <type>netcdf</type>
	  <basename>cloudshock_cube</basename>
	</writer>
      </stat>


      <stat>
	<name>
	  structure_cube
	</name>
	<numberOfSaves>2</numberOfSaves>
	<numberOfH>4</numberOfH>
	<p>2</p>
	<writer>
	  <type>netcdf</type>
	  <basename>cloudshock_cube</basename>
	</writer>
      </stat>


      <stat>
	<name>
	  structure_cube
	</name>
	<numberOfSaves>2</numberOfSaves>
	<numberOfH>4</numberOfH>
	<p>3</p>
	<writer>
	  <type>netcdf</type>
	  <basename>cloudshock_cube</basename>
	</writer>
      </stat>
    </stats>
  </uq>
</config>
